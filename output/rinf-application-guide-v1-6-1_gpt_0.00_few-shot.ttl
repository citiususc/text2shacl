era-sh:atoGradeAutomation a sh:PropertyShape ; 
    era:affectedClass era:CommonCharacteristicsSubset ; 
    era:affectedProperty era:atoGradeAutomation ; 
    era:rinfIndex "1.1.1.3.13.1" ; 
    era:scope "local" ; 
    rdfs:comment "ATO grade of automation installed lineside."@en ; 
    sh:nodeKind sh:IRI ; 
    sh:maxCount 1 ; 
    sh:message "atoGradeAutomation (1.1.1.3.13.1): The ATO grade of automation must be provided as a reference to a concept."@en ; 
    sh:path era:atoGradeAutomation ; 
    sh:severity sh:Violation . 

era-sh:atoGradeAutomationApplicability a sh:SPARQLConstraint ; 
    era:affectedClass era:CommonCharacteristicsSubset ; 
    era:affectedProperty era:atoGradeAutomation ; 
    era:rinfIndex "1.1.1.3.13.1" ; 
    era:scope "local" ; 
    rdfs:comment "Parameter only applicable when ETCS Baseline > 4 MR1 and ATO is implemented."@en ; 
    sh:message "atoGradeAutomationApplicability (1.1.1.3.13.1): The ATO grade of automation is only applicable if the ETCS Baseline is greater than 4 MR1 and ATO is implemented."@en ; 
    sh:select """ 
        PREFIX era: <http://data.europa.eu/949/> 
        SELECT DISTINCT $this WHERE { 
            $this era:etcsBaseline ?baseline . 
            FILTER(?baseline > 4) 
            $this era:atoImplementation ?ato . 
            FILTER(?ato = "True") 
        } 
    """ ; 
    sh:severity sh:Violation .

era-sh:belongsTo a sh:PropertyShape ; 
