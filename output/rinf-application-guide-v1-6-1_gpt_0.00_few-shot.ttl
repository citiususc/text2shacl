era-sh:belongsToApplicability a sh:SPARQLConstraint ; 
    era:affectedClass era:InfrastructureElement ; 
    era:affectedProperty era:belongsTo ; 
    era:rinfIndex "4.6.4" ; 
    era:scope "local" ; 
